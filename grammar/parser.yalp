
/* Definicion de parser */

/* seccion de tokens */

%token TOKEN_SENTENCE
%token TOKEN_OR
%token TOKEN_AND
%token TOKEN_R_BRACE
%token TOKEN_L_BRACE
IGNORE WS

/* finaliza seccion de tokens /*

%%

/* seccion de producciones*/
S:
    S TOKEN_AND P
    | P
;
P:
    P TOKEN_OR Q
    | Q
;
Q:
    TOKEN_L_BRACE S TOKEN_R_BRACE
    | TOKEN_SENTENCE
;
/* finaliza seccion de producciones */